<script>
  document.addEventListener('DOMContentLoaded', () => {
    const swiper = new Swiper('.swiper', {
      pagination: {
        el: '.swiper-pagination',
      },
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
      },
    });

    // open room size popup
    let btnPopupRoom = document.querySelector('.build-system .js-popup-room');
    btnPopupRoom.addEventListener('click', () => {
      document.querySelector('.popup__room-size').classList.add('popup--opened');
      document.querySelector('.room-size__wrapper .bs__overlay').classList.add('popup--opened');
    });
    document.querySelector('.room-size__wrapper .bs__overlay').addEventListener('click', () => {
      document.querySelector('.popup__room-size').classList.remove('popup--opened');
      document.querySelector('.room-size__wrapper .bs__overlay').classList.remove('popup--opened');
    });
    document.querySelector('.popup__icon-close').addEventListener('click', () => {
      document.querySelector('.popup__room-size').classList.remove('popup--opened');
      document.querySelector('.bs__overlay').classList.remove('popup--opened');
    });

    // open when scroll
    let elem = document.querySelector(".room-size__wrapper");
    if (elem ) {
      window.addEventListener('scroll', () => {
        let topCoord = elem.getBoundingClientRect().top;
        if ((topCoord < -800) && (sessionStorage.getItem('room_popup') == null) && (sessionStorage.getItem('room_popup') !== 'false')) { 
          sessionStorage.setItem('room_popup', 'false');
          document.querySelector('.popup__room-size').classList.add('popup--opened');
          document.querySelector('.room-size__wrapper .bs__overlay').classList.add('popup--opened');
        }
      });
    }

    // open skate-options-popup
    document.querySelector('.skate-options--open-popup').addEventListener('click', () => {
      document.querySelector('.popup__skate-options').classList.add('popup--opened');
      document.querySelector('.skate-options__wrapper .bs__overlay').classList.add('popup--opened');
    });
    document.querySelector('.skate-options__wrapper .bs__overlay').addEventListener('click', () => {
      document.querySelector('.popup__skate-options').classList.remove('popup--opened');
      document.querySelector('.skate-options__wrapper .bs__overlay').classList.remove('popup--opened');
    });
    document.querySelector('.skate-options__wrapper .popup__icon-close').addEventListener('click', () => {
      document.querySelector('.popup__skate-options').classList.remove('popup--opened');
      document.querySelector('.skate-options__wrapper .bs__overlay').classList.remove('popup--opened');
    });

    // open share-popup
    document.querySelector('.bs__share').addEventListener('click', () => {
      document.querySelector('.popup__share').classList.add('popup--opened');
      document.querySelector('.bs__total-wrapper .bs__overlay').classList.add('popup--opened');
    });
    document.querySelector('.bs__total-wrapper .bs__overlay').addEventListener('click', () => {
      document.querySelector('.popup__share').classList.remove('popup--opened');
      document.querySelector('.bs__total-wrapper .bs__overlay').classList.remove('popup--opened');
    });
    document.querySelector('.bs__total-wrapper .popup__icon-close').addEventListener('click', () => {
      document.querySelector('.popup__share').classList.remove('popup--opened');
      document.querySelector('.bs__total-wrapper .bs__overlay').classList.remove('popup--opened');
    });

    //get room size
    let USDollar = new Intl.NumberFormat('en-US', {
        style: 'currency',
        currency: 'USD',
    });
    let dimensions = {};
    let roomSizeWrapper = document.querySelector('.popup__room-size');
    let amountRails = 1;
    document.querySelectorAll('.measurement-system').forEach(item => {
      item.addEventListener('click', () => {
        if (item.classList.contains("m-s__imperial")) {
          document.querySelector('.measurement-system__output').innerHTML = "In yard";
        } else if (item.classList.contains("m-s__metric")) {
          document.querySelector('.measurement-system__output').innerHTML = "In meter"
        }
      });
    });
    roomSizeWrapper.querySelector('.js-room-size--submit').addEventListener('click', (e) => {
      event.preventDefault();
      dimensions.lengthRoom = roomSizeWrapper.querySelector('.input__length').value;
      dimensions.widthRoom = roomSizeWrapper.querySelector('.input__width').value;
      roomSizeWrapper.querySelectorAll('.measurement-system').forEach((item) => {
        if (item.checked) {
          dimensions.measurement = item.value;
        }
      });
      if (dimensions.measurement == 'metric') {
        amountRails = ((dimensions.lengthRoom * dimensions.widthRoom) / {{ section.settings.area_1_rails }}).toFixed();
        btnPopupRoom.querySelector('input[type=radio]').value = amountRails;
        btnPopupRoom.querySelector('.bs__title-subtitle span span').innerHTML = amountRails;
        btnPopupRoom.querySelector('.bs__label-price span').innerHTML = (btnPopupRoom.querySelector('.bs__label-price').dataset.price * amountRails).toFixed(2);
        document.querySelectorAll('.bs__total-wrapper  .total__wrapper-area').forEach((item) => {
          item.innerHTML = `${dimensions.lengthRoom * dimensions.widthRoom} m<sup>2</sup>`;
          item.style.lineHeight = '25px';
        });
        document.querySelectorAll('.bs__total-wrapper .js-bs__label-price').forEach((item) => {
          item.innerHTML = `${dimensions.lengthRoom * dimensions.widthRoom} m<sup>2</sup>`;
          item.style.lineHeight = '25px';
        });
        document.querySelectorAll('.plant-count').forEach((item) => {
          document.querySelectorAll('.plant-capacity__wrapper .js-pots').forEach((itemPots) => {
            if (itemPots.querySelector('input').checked) {
              item.innerHTML = ((itemPots.dataset.count * itemPots.dataset.plant_count_pot) * amountRails).toFixed();
            }
          });
        });
        document.querySelector('.bs__price-delivery h3').innerHTML = `USD  ${USDollar.format((document.querySelector('.bs__price-delivery h3').dataset.clear_price * amountRails).toFixed(2)).replace("$", "")}`
      } else if (dimensions.measurement == 'imperial') {
        amountRails = ((dimensions.lengthRoom * dimensions.widthRoom) / {{ section.settings.area_1_rails }}).toFixed();
        btnPopupRoom.querySelector('input[type=radio]').value = amountRails;
        btnPopupRoom.querySelector('.bs__title-subtitle span span').innerHTML = amountRails;
        btnPopupRoom.querySelector('.bs__label-price span').innerHTML = (btnPopupRoom.querySelector('.bs__label-price').dataset.price * amountRails).toFixed(2);
        document.querySelectorAll('.bs__total-wrapper  .total__wrapper-area').forEach((item) => {
          item.innerHTML = `${dimensions.lengthRoom * dimensions.widthRoom} sf`;
        });
        document.querySelectorAll('.bs__total-wrapper .js-bs__label-price').forEach((item) => {
          item.innerHTML = `${dimensions.lengthRoom * dimensions.widthRoom} sf`;
        });
        document.querySelectorAll('.plant-count').forEach((item) => {
          document.querySelectorAll('.plant-capacity__wrapper .js-pots').forEach((itemPots) => {
            if (itemPots.querySelector('input').checked) {
              item.innerHTML = ((itemPots.dataset.count * itemPots.dataset.plant_count_pot) * amountRails).toFixed();
            }
          });
        });
        document.querySelector('.bs__price-delivery h3').innerHTML = `USD  ${USDollar.format((document.querySelector('.bs__price-delivery h3').dataset.clear_price * amountRails).toFixed(2)).replace("$", "")}`
      }
      document.querySelector('.popup__room-size').classList.remove('popup--opened');
      document.querySelector('.room-size__wrapper .bs__overlay').classList.remove('popup--opened');
    });

    // Pots
    document.querySelectorAll('.plant-capacity__wrapper .js-pots').forEach((item) => {
      if (item.querySelector('input').checked) {
        document.querySelectorAll('.plant-count').forEach((item__) => {
          item__.innerHTML = (item.dataset.count * item.dataset.plant_count_pot).toFixed();
        });
      }
      item.addEventListener('click', () => {
        if (item.querySelector('input').checked) {
          let railsPot = amountRails * item.dataset.count;
          document.querySelectorAll('.plant-count').forEach((item_) => {
            item_.innerHTML = (railsPot * item.dataset.plant_count_pot).toFixed();
          });
        }
      });
    });

    // Change price in first time
    let newPrice = 0;
    document.querySelectorAll('.js-input-product').forEach((item) => {
      if (item.checked) {
        newPrice = newPrice + +item.dataset.product_price;
      }
    });
    document.querySelector('.bs__price-delivery h3').innerHTML = `USD ${(newPrice * amountRails).toFixed(2)}`;
    document.querySelector('.bs__price-delivery h3').dataset.clear_price = `${(newPrice * amountRails).toFixed(2)}`;
    
    // Change price in next time
    let newPriceClick = 0;
    document.querySelectorAll('.js-input-product').forEach((item) => {
      item.addEventListener('click', () => {
        document.querySelectorAll('.js-input-product:checked').forEach((item_) => {
          newPriceClick = newPriceClick + +item_.dataset.product_price;
          document.querySelector('.bs__price-delivery h3').innerHTML = `USD ${(newPriceClick * amountRails).toFixed(2)}`;
        });
        newPriceClick = 0;
      });
    });

    // open day picker
    let notesOrderDays = "";
    let dataPicker = document.querySelectorAll('.js-delivery-dates');
    dataPicker.forEach((item) => {
      item.addEventListener('click', (e) => {
        e.preventDefault();
        item.nextElementSibling.showPicker();
        item.nextElementSibling.addEventListener("change", () => {
          console.log(item.nextElementSibling.value);
          notesOrderDays = item.nextElementSibling.value;
        })
      }); 
    });

    // send to cart
    let btnMainSubmit = document.querySelector('.build-system .btn-main-submit');
    let inputArray = document.querySelectorAll('.build-system .js-label:not(.js-popup-room) input');
    let formData = { items: [] };
    btnMainSubmit.addEventListener('click', (e) => {
      e.preventDefault();
      inputArray.forEach((item) => {
        if (item.checked) {
          formData.items.push({ id: item.value, quantity: amountRails });
        }
      });
      fetch(window.Shopify.routes.root + 'cart/add.js', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify(formData),
      })
        .then((response) => {
          return response.json();
        })
        .then((res) => {
          fetch(window.Shopify.routes.root + 'cart/update.js', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json'
            },
            body: JSON.stringify({
              note: `${notesOrderDays}`
            })
          })
          .then(response => response.json())
          .then(data => {
            //console.log(data);
            window.location.reload();
          });
        })
        .catch((error) => {
          console.error('Error:', error);
        });
    });
  });
</script>

