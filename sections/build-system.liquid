              {%- endif -%}
            </div>
          </div>
        </article>

        <h2>Skate options. <span>Choose skates with or without handles</span></h2>
        {%- for product in section.settings.s_o_products -%}
          <label class="bs__label-{{ forloop.index }} js-label">
            <input
              class="js-input-product"
              data-product_price="{{ product.variants.first.price | money_without_currency }}"
              type="radio"
              name="s_o_products"
              value="{{ product.variants.first.id }}"
              {%- if forloop.index == 1 -%}
                checked
              {%- endif -%}
            >
            <p class="bs__title-subtitle">
              {{ product.title }}
              <span>{{ product.metafields.custom.bs_scate_options_subtitle | metafield_tag }}</span>
            </p>
            <p class="bs__label-price">
              {% if product.price > 0 %}
