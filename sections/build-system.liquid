{{ 'build-system.out.css' | asset_url | stylesheet_tag }}

{%- style -%}
  .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
      margin-top: {{ section.settings.margin_top }}px;
      margin-bottom: {{ section.settings.margin_bottom }}px;
  }
{%- endstyle -%}

<div class="build-system__wrapper section-{{ section.id }}-padding">
  <form>
    <div class="bs-section room-size__wrapper page-width">
      <div class="bs__images-wrapp">
        {%- if section.settings.room_size_image != blank -%}
          {{ section.settings.room_size_image | image_url: width: 820 | image_tag: class: '__image', loading: 'lazy' }}
        {%- endif -%}
      </div>
      <div class="bs__buttons-wrapp">
        <span class="bs__overlay"></span>
        <article class="popup__room-size">
          <span class="popup__icon-close">
            <svg xmlns="http://www.w3.org/2000/svg" width="14" height="14" viewBox="0 0 14 14" fill="none">
              <path d="M1.4 14L0 12.6L5.6 7L0 1.4L1.4 0L7 5.6L12.6 0L14 1.4L8.4 7L14 12.6L12.6 14L7 8.4L1.4 14Z" fill="#8F9394"/>
            </svg>
          </span>
          <h3>Measure your grow room.</h3>
          {%- if section.settings.r_s_popup_image != blank -%}
            {{ section.settings.r_s_popup_image | image_url: width: 820 | image_tag: class: '__image', loading: 'lazy' }}
          {%- endif -%}
          <form>
            <fieldset style="background: #F3F4F6;">
              <h2>Measurement system. <span>Choose your preference</span></h2>
              <div class="popup__wrapper-btn">
                <label class="popup-label button--primary">Imperial<input class="measurement-system m-s__imperial" type="radio" name="measurement" value="imperial"></label>
                <label class="popup-label button--secondary">Metric<input class="measurement-system m-s__metric" type="radio" name="measurement" value="metric" checked></label>
              </div>
            </fieldset>
            <fieldset style="align-items: baseline;">
              <h2>Enter Dimensions. <span class="measurement-system__output">In meters</span></h2>
              <div class="popup__wrapper-btn">
                <div class="popup__wrapper-btn--inline">
                  <input
                    class="input__length"
                    type="number"
                    placeholder="Length"
                    value=""
                    min="1"
                    step="1"
                    required
                    onfocus="this.placeholder = ''"
                    onblur="this.placeholder = 'Length'"
                    oninput="this.value = this.value.replace(/[^\d-]/g, '');"
                  >
                  <svg xmlns="http://www.w3.org/2000/svg" width="10" height="13" viewBox="0 0 10 13" fill="none">
                    <path d="M3.968 6.44148L0.208 0.889476H2.032L4.88 5.35348L7.856 0.889476H9.552L5.776 6.44148L9.792 12.3135H7.936L4.864 7.59348L1.696 12.3135H0L3.968 6.44148Z" fill="#8F9394"/>
                  </svg>
                  <input
                    class="input__width"
                    type="number"
                    placeholder="Width"
                    value=""
                    min="1"
                    step="1"
                    required
                    onfocus="this.placeholder = ''"
                    onblur="this.placeholder = 'Width'"
                    oninput="this.value = this.value.replace(/[^\d-]/g, '');"
                  >
                </div>
                <button class="button button--primary js-room-size--submit">Continue</button>
              </div>
            </fieldset>
          </form>
        </article>
        <h2>Room size. <span>Enter dimensions</span></h2>
        <label class="js-popup-room js-label">
          <input type="radio" name="roomsize" value="">
          <p class="bs__title-subtitle">
            Enter room size
            <span>
              <span>0</span>
              bays requried
            </span>
          </p>
          <p class="bs__label-price" data-price="{{- section.settings.price_1_rails -}}">
            USD <span style="display: contents;">{{- section.settings.price_1_rails -}}</span>
          </p>
        </label>
      </div>
    </div>

